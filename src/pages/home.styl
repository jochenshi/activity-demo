.home_page{
  width 100%
  height 100%
  .mint-header{
    z-index 11
  }
  .home_content{
    height 100%
    padding 40px 0 50px 0
    box-sizing border-box
    //border 1px solid
    border-top none
    overflow-y auto
    overflow-x hidden
    .top_slide{
      .s_carousel_item img{
        width 100%
        height 100%
      }
    }
    .body_content{
      header{
        height 36px
        border-bottom 1px solid #e3e3e3
        width 100%
        background-color #ffffff
        &.header_fixed{
          position fixed
          top 40px
        }
        div{
          float right
          width 50%
          line-height 36px
          span{
            display inline-block
            line-height 34px
            padding 0 8px
            &.is_active{
              color red
              border-bottom 2px solid red
            }
          }
        }
      }
      .list_content{
        margin 5px 5px 0 5px
      }
    }
  }
  .modified_tabbar{
    .mint-tab-item-label{
      line-height 3
    }
  }
}
